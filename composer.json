{
	"name": "robindort/preorder-timer",
	"type": "contao-bundle",
	"description": "A bundle to add a preorder functionality to Contao Isotope eCommerce checkout",
	"homepage": "https://github.com/RobinDort/preorder-timer",
	"version": "1.0",
	"license": "LGPL-3.0-or-later",
	"autoload": {
		"psr-4": {
			"RobinDort\\PreorderTimer\\": "src/"
		}
	},
	"authors": [
		{
			"name": "Robin Dort",
			"homepage": "https://www.alexanderdort.com"
		}
	],
	"require": {
		"php": "^8.2",
		"contao/core-bundle": "^4.13",
		"isotope/isotope-core": "^2.9.0",
		"umulmrum/holiday": "^0.11.0"
	},
	"require-dev": {
		"contao/manager-plugin": "^2.0"
	},
	"conflict": {
		"contao/manager-plugin": "<2.0 || >=3.0"
	},
	"extra": {
		"contao-manager-plugin": "RobinDort\\PreorderTimer\\ContaoManager\\Plugin"
	}
}
